<view><view class="cu-bar bg-white search"><view class="search-form radius"><text class="cuIcon-search"></text><block wx:if="{{searchID}}"><input adjust-position="{{false}}" type="text" placeholder="按商户名称或ID搜索" confirm-type="search" data-event-opts="{{[['input',[['__set_model',['','searchID','$event',[]]]]]]}}" value="{{searchID}}" bindinput="__e"/></block><input adjust-position="{{false}}" type="text" placeholder="按商户名称或ID搜索" confirm-type="search" data-event-opts="{{[['input',[['__set_model',['','searchname','$event',[]]]]]]}}" value="{{searchname}}" bindinput="__e"/></view><view data-event-opts="{{[['tap',[['searchbtn',['$event']]]]]}}" class="action" bindtap="__e"><button class="cu-btn bg-yellow shadow-blur rounded-12">搜索</button></view></view><text class="ml-3 font-24 text-gray">{{"共有"+merchantList.length+"户商家"}}</text><block wx:for="{{merchantList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['toshopEarn',['$0'],[[['merchantList','',index]]]]]]]}}" class="mt-2 mx-25 d-flex a-center border shadow-nom bg-white mb-2" style="height:180rpx;" bindtap="__e"><image class="ml-3 bg-blue" style="width:120rpx;height:120rpx;" src="{{item.logo_url}}"></image><view class="d-flex flex-column ml-3 font-24 text-black"><text>{{item.merchant_name}}</text><text>{{"电话："+(item.merchant_tel==null?'':item.merchant_tel)}}</text><text>{{"地址："+(item.merchant_address==null?'':item.merchant_address)}}</text></view></view></block></view>